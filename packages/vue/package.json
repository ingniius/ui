{
    "name": "@iueev/vue",
    "type": "module",
    "version": "0.0.0",
    "author": "@omaucf <omaucf@pm.me>",
    "license": "MIT",
    "homepage": "https://github.com/ingniius/ui#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ingniius/ui.git",
        "directory": "packages/vue"
    },
    "bugs": {
        "url": "https://github.com/ingniius/ui/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "imports": {
        "#build/ui/*": "./.nuxt/ui/*.ts",
        "#build/ui.css": "./.nuxt/ui.css"
    },
    "exports": {
        "./runtime/*": "./dist/runtime/*",
        "./components/*": "./dist/runtime/components/ui/*",
        "./color-mode/*": "./dist/runtime/components/vendor/color-mode/*",
        "./prose/*": "./dist/runtime/components/vendor/prose/*",
        "./composables/*": "./dist/runtime/composables/*",
        "./types": {
            "types": "./dist/runtime/types/index.d.ts",
            "import": "./dist/runtime/types/index.js"
        },
        "./utils/*": {
            "types": "./dist/runtime/utils/*.d.ts",
            "import": "./dist/runtime/utils/*.js"
        },
        "./defaults": {
            "types": "./dist/defaults.d.mts",
            "import": "./dist/defaults.mjs"
        },
        "./nuxt": {
            "types": "./dist/module.d.mts",
            "import": "./dist/module.mjs"
        },
        ".": {
            "types": "./dist/index.d.mts",
            "style": "./dist/runtime/ui.css",
            "import": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "style": "./dist/runtime/ui.css",
    "main": "./dist/index.mjs",
    "typesVersions": {
        "*": {
            "runtime/*": [
                "./dist/runtime/*"
            ],
            "components/*": [
                "./dist/runtime/components/ui/*"
            ],
            "color-mode/*": [
                "./dist/runtime/components/vendor/color-mode/*"
            ],
            "prose/*": [
                "./dist/runtime/components/vendor/prose/*"
            ],
            "composables/*": [
                "./dist/runtime/composables/*"
            ],
            "types": [
                "./dist/runtime/types/index.d.ts"
            ],
            "utils/*": [
                "./dist/runtime/utils/*.d.ts"
            ],
            "defaults": [
                "./dist/defaults.d.mts"
            ],
            "nuxt": [
                "./dist/module.d.mts"
            ],
            ".": [
                "./dist/index.d.mts"
            ]
        }
    },
    "files": [
        ".nuxt/ui",
        ".nuxt/ui.css",
        "LICENSE",
        "README.md",
        "dist",
        "package.json"
    ],
    "scripts": {
        "prepare": "[ ! -d dist ] && run-s dev || true",
        "dev": "nuxt-module-build build --stub",
        "build": "nuxt-module-build build",
        "lint": "eslint . --fix --cache --cache-location .cache/.eslintcache",
        "typecheck": "nuxi typecheck --noEmit",
        "clean": "rimraf .cache .data .nuxt .turbo dist node_modules",
        "postinstall": "[ ! -d .nuxt ] && nuxi prepare || true"
    },
    "peerDependencies": {
        "nuxt": ">=3",
        "vue": ">=3"
    },
    "peerDependenciesMeta": {
        "nuxt": {
            "optional": true
        },
        "vue": {
            "optional": true
        }
    },
    "dependencies": {
        "@nuxt/icon": "^1.15.0",
        "@tailwindcss/postcss": "^4.1.11",
        "@tailwindcss/vite": "catalog:",
        "@vueuse/core": "^13.5.0",
        "defu": "^6.1.4",
        "es-toolkit": "^1.39.6",
        "knitwork": "^1.2.0",
        "reka-ui": "^2.3.2",
        "scule": "^1.3.0",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "catalog:",
        "ufo": "^1.6.1"
    },
    "optionalDependencies": {
        "@nuxt/fonts": "*",
        "@nuxt/image": "*",
        "@nuxtjs/color-mode": "*",
        "@nuxtjs/mdc": "*"
    },
    "devDependencies": {
        "nuxt": "catalog:",
        "vue": "catalog:"
    }
}
