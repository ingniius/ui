{
  "$schema": "../node_modules/@biomejs/biome/configuration_schema.json",
  "extends": ["ultracite/vue"],
  "overrides": [
    {
      "includes": ["**/*.vue"],
      "linter": {
        "rules": {
          "correctness": {
            "noUnusedVariables": "off",
            "noUnusedImports": "off"
          },
          "style": {
            "useImportType": "off"
          }
        }
      }
    }
  ]
}
